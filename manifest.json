{
    "manifest_version": 3,
    "name": "AI2Fumo",
    "description": "Browser extension that replaces webpage mentions of AI with Fumo",
    "version": "0.1.0",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "index.js"
            ],
            "matches": [
                "*"
            ]
        }
    ]
}